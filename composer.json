{
    "name": "anormaly/jetstream-company",
    "description": "Just Laravel Jetstream with companies instead of teams.",
    "keywords": ["laravel", "tailwind", "auth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/knightkill/jetstream-company/issues",
        "source": "https://github.com/knightkill/jetstream-company"
    },
    "authors": [
        {
            "name": "Hardip Patel",
            "email": "hardip@anormaly.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "illuminate/support": "^8.0",
        "jenssegers/agent": "^2.6",
        "league/commonmark": "^1.3",
        "laravel/fortify": "^1.6.1"
    },
    "require-dev": {
        "inertiajs/inertia-laravel": "^0.3",
        "laravel/sanctum": "^2.6",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Jetstream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Jetstream\\Tests\\": "tests/",
            "App\\": "stubs/app/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Jetstream\\JetstreamServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
